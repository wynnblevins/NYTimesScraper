<div id="articlesWrapper"></div>

<!-- Added Articles Modal -->
<div class="modal fade" id="notesModal" tabindex="-1" 
  role="dialog" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-body">
        <form class="form-horizontal">
          <div class="form-group">
            <label for="text" class="control-label col-xs-4">Text Field</label> 
            <div class="col-xs-8">
              <div class="input-group">
                <input id="noteField" id="text" name="text" type="text" class="form-control">
              </div>
            </div>
          </div> 
          <div class="form-group row">
            <div class="col-xs-offset-4 col-xs-8">
              <button name="submit" type="button" class="btn btn-primary" 
                id="addNoteButton">Add Note</button>
            </div>
          </div>
        </form>
      </div>
      <div class="modal-footer">
        <button id="submitNotesButton" type="button" 
          data-dismiss="modal" class="btn btn-primary">OK</button>
      </div>
    </div>
  </div>
</div>

<script src="../public/assets/scripts/articles-page.js"></script>